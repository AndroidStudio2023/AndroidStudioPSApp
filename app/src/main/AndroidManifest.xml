<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/appicon1"
        android:label="@string/new_app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.PhysiotherapyCenterApplication"
        tools:targetApi="31">
        <activity
            android:name=".HistoryPage"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

        <activity
            android:name=".CreateServicePage"
            android:exported="false" />
        <activity
            android:name=".ServicesCentralPage"
            android:exported="false" />
        <activity
            android:name=".new_patient"
            android:exported="false" />
        <activity
            android:name=".main_doctor_page"
            android:exported="false" />
        <activity
            android:name=".NewPSFCentralPage"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreatePSPage"
            android:exported="false" />
        <activity
            android:name=".PSFCentralPage"
            android:exported="false" />
        <activity
            android:name=".LoginPageActivity"
            android:exported="false" />
    </application>

</manifest>